{
  "rules": {
    "editorconfig": {
      "charset": false
    },
    "no-dead-link": {
      "checkRelative": false,
      "baseURI": null,
      "ignore": [
        "*api.flexera.*",
        "*app.flexera.*",
        "*login.flexera.*",
        "*flexnetmanager.com*",
        "*flexeraeng.com*",
        "*rightscale.com*"
      ],
      "preferGET": [],
      "ignoreRedirects": true,
      "retry": 1,
      "userAgent": "textlint-rule-no-dead-link/1.0"
    }
  },
  "filters": {
    "comments": true
  }
}
